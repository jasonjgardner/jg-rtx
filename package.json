{
  "name": "jg-rtx",
  "version": "0.15.0",
  "description": "Minecraft raytracing-enabled resource pack",
  "private": true,
  "type": "module",
  "scripts": {
    "copy": "cd scripts & install.cmd",
    "bake": "node scripts/halfBaked.js",
    "manifest": "node scripts/manifest.js",
    "pack": "node scripts/mcpack.js",
    "play": "npm run bake & npm run copy",
    "update": "npm run bake & npm run manifest",
    "release": "npm run update & npm run pack",
    "mineways:channelmixer": ".\\bin\\TileMaker\\ChannelMixer.exe -i .\\development_resource_packs\\JG-RTX -o .\\dist\\JG-RTX-channelmixer",
    "mineways:tilemaker": ".\\bin\\TileMaker\\TileMaker.exe -i .\\bin\\TileMaker\\terrainBase.png  -d .\\dist\\JG-RTX-channelmixer -o .\\dist\\JG-RTX-mineways\\terrainExt_JG-RTX.png -t 256",
    "mineways": "npm run mineways:channelmixer & npm run mineways:tilemaker",
    "log:mineways": "npm run mineways > logs/mineways.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonjgardner/jg-rtx.git"
  },
  "keywords": [
    "minecraft",
    "rtx",
    "raytracing",
    "resource-pack",
    "jgrtx",
    "jg-rtx"
  ],
  "author": {
    "name": "Jason J. Gardner",
    "email": "howdy@jasongardner.co",
    "url": "https://jasongardner.co"
  },
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/jasonjgardner/jg-rtx/issues"
  },
  "homepage": "https://github.com/jasonjgardner/jg-rtx#readme",
  "os": [
    "win32"
  ],
  "engines": {
    "node": "^15.7.0",
    "vscode": "^1.22.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "canvas": "^2.7.0",
    "colors": "^1.4.0",
    "glob": "^7.1.6",
    "merge-images": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
